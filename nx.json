{
  "npmScope": "mda",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "backend-api": {
      "tags": []
    },
    "frontend-admin": {
      "tags": [
        "scope:frontend-admin",
        "type:app"
      ]
    },
    "frontend-admin-e2e": {
      "tags": [],
      "implicitDependencies": [
        "frontend-admin"
      ]
    },
    "frontend-client": {
      "tags": [
        "scope:frontend-client",
        "type:app"
      ]
    },
    "frontend-client-e2e": {
      "tags": [],
      "implicitDependencies": [
        "frontend-client"
      ]
    },
    "shared-data-access-interfaces": {
      "tags": []
    },
    "shared-feature-feature": {
      "tags": []
    },
    "shared-ui-ui": {
      "tags": []
    },
    "shared-utility-utility": {
      "tags": []
    },
    "ui-component": {
      "tags": [
        "scope:ui-lib",
        "type:lib"
      ]
    }
  }
}
